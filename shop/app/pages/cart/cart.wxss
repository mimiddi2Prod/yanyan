/* 模态框 */

.modal {
  position: fixed;
  height: 100%;
  width: 100%;
  z-index: 3;
}

.modal-bg {
  position: absolute;
  height: 100%;
  width: 100%;
  background-color: #3e3e3e;
  opacity: 0.8;
}

.modal-container {
  position: absolute;
  width: 80%;
  height: auto;
  top:18%;
  left: 10%;
  background-color: #fff;
  border-radius: 8rpx;
}

.modal-container>view{
  padding: 20rpx;
  border-bottom: 1rpx solid #f4f4f4;
}

/* .region{
  margin: 20rpx 0;
} */

.modal-container>view>textarea{
  width: 100%;
  height: 100rpx;
}

.modal-container>button{
  margin: 20rpx;
  color: #fff;
  border: 1rpx solid #434343;
  background-color: #434343;
  font-size: 28rpx;
}

.modal-title{
  font-size: 32rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
}

.modal-container>image{
  width:60rpx;
  height:60rpx;
  position: absolute;
  top:-20rpx;
  right: -20rpx;
  border-radius: 50%;
}

