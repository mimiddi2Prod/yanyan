.sanjiao:after, .sanjiao:before {
  border: solid transparent;
  content: ' ';
  height: 0;
  top: 99%;
  position: absolute;
  width: 0;
}

.sanjiao:before {
  border-width: 8px;
  /* border-right-color: #fdd2e4; */
  border-top-color: green;
}

.sanjiao-time:after, .sanjiao-time:before {
  border: solid transparent;
  content: ' ';
  height: 0;
  top: 99%;
  position: absolute;
  width: 0;
}

.sanjiao-time:before {
  border-width: 8px;
  /* border-right-color: #fdd2e4; */
  border-top-color: blue;
}
